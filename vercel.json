{
  "version": 2,
  "routes": [
    {
      "src": "/api",
      "methods": ["GET"],
      "dest": "/api"
    },
    {
      "src": ".*",
      "dest": "/index.js"
    }
  ],
  "builds": [
    {
      "src": "/api/**",
      "use": "@now/node"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "api/**"
    }
  }
}
